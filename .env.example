# GROOT Environment Variables
# Copy this file to .env and fill in your values

# Required: Your Anthropic API key
ANTHROPIC_API_KEY=your-api-key-here

# Optional: Model to use (defaults to claude-sonnet-4-20250514)
# GROOT_MODEL=claude-sonnet-4-20250514

# Optional: Enable debug mode
# GROOT_DEBUG=true

# Optional: Disable BEADS integration
# GROOT_BEADS_ENABLED=false

# Optional: Custom output directory
# GROOT_OUTPUT_DIR=./output

# Optional: Custom templates directory  
# GROOT_TEMPLATES_DIR=./templates